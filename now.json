{
    "version": 2,
    "name": "disqusjs-proxy",
    "alias": [
        "disqusjs.v2beach.cn"
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "https://disqus.com/api/$1"
        }
    ],
    "github": {
        "enabled": true,
        "autoJobCancelation": true
    }
}